    "gulp-uglify": "^1.2.0",
    "install": "^0.1.8",
    "jasmine-core": "^2.3.0",
    "jasmine-jquery": "^2.0.5",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "karma": "^0.12.33",
    "karma-chrome-launcher": "^0.1.8",
    "karma-coverage": "^0.2.6",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.2.0",
    "karma-junit-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^0.1.4",
